<Window x:Class="Lab05.ListEmp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab05"
        mc:Ignorable="d"
        Title="ListEmp" Height="350" Width="700">
    <Grid>
        <StackPanel>
            <Label Content="Lista de Empleados" FontSize="20" HorizontalAlignment="Center"/>
            <DataGrid x:Name="dgEmpleados" AutoGenerateColumns="True" CanUserAddRows="False" ColumnWidth="*" HorizontalAlignment="Center" VerticalAlignment="Center" Width="600"/>
        </StackPanel>
    </Grid>
</Window>
